IMPLEMENTATION MchWithConstants_i
REFINES MchWithConstants

VALUES
    max_count = 10

OPERATIONS
    res <-- opMchWithCConstants(nn)=
    BEGIN
        IF nn < max_count THEN
            res := TRUE
        ELSE
            res := FALSE
        END
    END

 END