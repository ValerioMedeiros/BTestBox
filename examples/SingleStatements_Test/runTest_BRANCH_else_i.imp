IMPLEMENTATION
    runTest_BRANCH_else_i

REFINES
    runTest_BRANCH_else

IMPORTS
    TestSet_BRANCH_else

LOCAL_OPERATIONS
    verdict <-- testopelse =
        ANY kk WHERE kk : BOOL THEN verdict := kk END

OPERATIONS
    verdict <-- testopelse =
    BEGIN
        VAR v0, v1 IN
            v0 <-- TEST_0_opelse;
            v1 <-- TEST_1_opelse;
            IF v0 = TRUE & v1 = TRUE THEN
                verdict := TRUE
            ELSE
                verdict := FALSE
            END
        END
    END;

    verdict <-- testAll =
    BEGIN
        VAR v0 IN
            v0 <-- testopelse;
            IF v0 = TRUE THEN
                verdict := TRUE
            ELSE
                verdict := FALSE
            END
        END
    END
END