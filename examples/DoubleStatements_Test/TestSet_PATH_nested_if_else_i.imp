IMPLEMENTATION
    TestSet_PATH_nested_if_else_i

REFINES
    TestSet_PATH_nested_if_else

IMPORTS
    nested_if_else

OPERATIONS
    verdict <-- TEST_0_opnested_if_else =
    BEGIN
        VAR aux1, aux2 IN
            aux1, aux2 <-- opnested_if_else(0, 5);
            IF aux1 = 0 & aux2 = 5 THEN
                verdict := TRUE
            ELSE
                verdict := FALSE
            END
        END
    END;

