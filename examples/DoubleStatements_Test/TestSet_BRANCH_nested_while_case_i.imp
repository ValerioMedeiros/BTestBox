IMPLEMENTATION
    TestSet_BRANCH_nested_while_case_i

REFINES
    TestSet_BRANCH_nested_while_case

IMPORTS
    nested_while_case

OPERATIONS
    verdict <-- TEST_0_opnested_while_case =
    BEGIN
        VAR aux1, aux2 IN
            aux1, aux2 <-- opnested_while_case(1, 2);
            IF aux1 = 1 & aux2 = 2 THEN
                verdict := TRUE
            ELSE
                verdict := FALSE
            END
        END
    END;

    verdict <-- TEST_1_opnested_while_case =
    BEGIN
        VAR aux1, aux2 IN
            aux1, aux2 <-- opnested_while_case(1, 0);
            IF aux1 = 1 & aux2 = 0 THEN
                verdict := TRUE
            ELSE
                verdict := FALSE
            END
        END
    END;

    verdict <-- TEST_2_opnested_while_case =
    BEGIN
        VAR aux1, aux2 IN
            aux1, aux2 <-- opnested_while_case(1, 1);
            IF aux1 = 1 & aux2 = 1 THEN
                verdict := TRUE
            ELSE
                verdict := FALSE
            END
        END
    END
END