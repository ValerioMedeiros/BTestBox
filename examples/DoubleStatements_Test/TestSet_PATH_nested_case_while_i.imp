OPERATIONS
    verdict <-- TEST_0_opnested_case_while =
    BEGIN
        VAR aux1, aux2 IN
            aux1, aux2 <-- opnested_case_while(2, 0);
            IF aux1 = 2 & aux2 = 0 THEN
                verdict := TRUE
            ELSE
                verdict := FALSE
            END
        END
    END;

