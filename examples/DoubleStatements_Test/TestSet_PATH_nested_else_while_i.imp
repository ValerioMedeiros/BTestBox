IMPLEMENTATION
    TestSet_PATH_nested_else_while_i

REFINES
    TestSet_PATH_nested_else_while

IMPORTS
    nested_else_while

OPERATIONS
    verdict <-- TEST_0_opnested_else_while =
    BEGIN
        VAR aux1, aux2 IN
            aux1, aux2 <-- opnested_else_while(5, 0);
            IF aux1 = 5 & aux2 = 0 THEN
                verdict := TRUE
            ELSE
                verdict := FALSE
            END
        END
    END;

    verdict <-- TEST_1_opnested_else_while =
    BEGIN
        VAR aux1, aux2 IN
            aux1, aux2 <-- opnested_else_while(5, 1);
            IF aux1 = 5 & aux2 = 1 THEN
                verdict := TRUE
            ELSE
                verdict := FALSE
            END
        END
    END;

    verdict <-- TEST_2_opnested_else_while =
    BEGIN
        VAR aux1, aux2 IN
            aux1, aux2 <-- opnested_else_while(0, 0);
            IF aux1 = 0 & aux2 = 0 THEN
                verdict := TRUE
            ELSE
                verdict := FALSE
            END
        END
    END;

    verdict <-- TEST_3_opnested_else_while =
    BEGIN
        VAR aux1, aux2 IN
            aux1, aux2 <-- opnested_else_while(0, 1);
            IF aux1 = 0 & aux2 = 1 THEN
                verdict := TRUE
            ELSE
                verdict := FALSE
            END
        END
    END
END