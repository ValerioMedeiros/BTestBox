IMPLEMENTATION MchWithAConstants_i
REFINES MchWithAConstants

CONCRETE_CONSTANTS
    max_countConcrete

PROPERTIES
    max_countConcrete = max_count

VALUES
    max_countConcrete = 10

OPERATIONS
    res <-- opMchWithAConstants ( nn ) =
    BEGIN
        IF nn < max_countConcrete THEN
            res := TRUE
        ELSE
            res := FALSE
        END
    END

 END
