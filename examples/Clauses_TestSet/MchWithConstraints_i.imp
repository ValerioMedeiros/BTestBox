IMPLEMENTATION MchWithConstraints_i(max_count)
REFINES MchWithConstraints

OPERATIONS
    res <-- opMchWithConstraints(nn)=
    BEGIN
        IF nn < max_count THEN
            res := TRUE
        ELSE
            res := FALSE
        END
    END

 END