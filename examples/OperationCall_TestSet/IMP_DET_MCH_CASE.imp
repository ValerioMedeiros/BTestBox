IMPLEMENTATION IMP_DET_MCH_CASE
REFINES DET_MCH_CASE

OPERATIONS
    res <-- opDET_MCH_CASE(xx)=
    BEGIN
        CASE xx OF
            EITHER 0 THEN
                res := xx
            OR 1 THEN
                res := xx
            ELSE
                res := xx
            END
        END
    END

 END