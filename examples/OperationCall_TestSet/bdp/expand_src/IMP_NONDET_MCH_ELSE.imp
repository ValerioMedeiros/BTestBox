IMPLEMENTATION IMP_NONDET_MCH_ELSE
REFINES NONDET_MCH_ELSE

OPERATIONS
    res <-- opNONDET_MCH_ELSE ( xx , yy ) =
    BEGIN
        IF xx > yy THEN
            res := xx
        ELSE
            res := yy
        END
    END

 END
