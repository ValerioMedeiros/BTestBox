IMPLEMENTATION nested_if_else_i
REFINES nested_if_else

OPERATIONS
    res1 , res2 <-- opnested_if_else ( xx , yy ) =
    BEGIN
        IF xx < 5 THEN
            IF yy < 5 THEN
                res1 := 0 ; res2 := 0
            ELSE
                res1 := 0 ; res2 := yy
            END
        END
    END

 END
