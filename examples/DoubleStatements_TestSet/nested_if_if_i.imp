IMPLEMENTATION nested_if_if_i
REFINES nested_if_if

OPERATIONS
    res1, res2 <-- opnested_if_if(xx ,yy)=
    BEGIN
        IF xx < 5 THEN
            IF yy < 5 THEN
                res1 := 0; res2 := 0
            END
        END    
    END

 END